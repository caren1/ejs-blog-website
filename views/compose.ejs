<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="postCompose" action="/compose" method="post">

  <div class="form-group">
  <label for="postTitle">Title</label>
  <input type="text" name="postTitle" class="form-control" autocomplete="off">
  </div>

  <div class="form-group">
  <label for="postContent">Post</label>
  <textarea name="postBody" cols="30" rows="5" class="form-control"></textarea>
  </div>

  <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>



<%- include("partials/footer") -%>
